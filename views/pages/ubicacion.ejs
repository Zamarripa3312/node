<!DOCTYPE html>
<html lang="es" >
  <head>
    
    <%- include('../partials/head'); %>
    <script>
      window.onload = function() {
        L.mapquest.key = 'dNsEkFJLXTUqM5APneUXhwLeXcvDKARz';

        var map = L.mapquest.map('map', {
          center: [21.17, -100.93321],
          layers: L.mapquest.tileLayer('map'),
          zoom: 12
        });

        map.addControl(L.mapquest.control());
      }
      
    </script>
  </head>
  <body>
    <body style="border: 0; margin: 0;">
      <div id="map" style="width: 100%; height: 530px;"></div>
    </body>
    <%- include('../partials/navbar'); %>
    <div class="jumbotron jumbotron-fluid">
      <div class="container">
        <h1 class="display-4">Bienvenido a NodeJS & Express</h1>
        <p class="lead">Este es un ejemplo basico</p>
        <p>Mi primier sitio basico con Express</p>
      </div>
    </div>

     
    <%- include('../partials/script'); %>
  </body>
</html>
